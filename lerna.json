{
  "version": "1.0.0",
  "npmClient": "npm",
  "useWorkspaces": true,
  "packages": [
    "cdk",
    "web-ui"
  ],
  "command": {
    "version": {
      "exact": true,
      "conventionalCommits": true,
      "message": "chore(release): publish %s"
    },
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish %s"
    },
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    }
  }
}
